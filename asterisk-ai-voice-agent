#!/usr/bin/env python3
"""
Asterisk AI Voice Agent - CLI Entry Point

This script provides the main entry point for the Asterisk AI Voice Agent CLI.
"""

import sys
from pathlib import Path

# Add src directory to Python path
src_dir = Path(__file__).parent / "src"
sys.path.insert(0, str(src_dir))

from cli import app

if __name__ == "__main__":
    app()
